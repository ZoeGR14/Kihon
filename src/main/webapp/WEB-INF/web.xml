<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControlerJ</servlet-name>
        <servlet-class>Controlador.ControlerJ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerZ</servlet-name>
        <servlet-class>Controlador.ControlerZ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerZz</servlet-name>
        <servlet-class>Controlador.ControlerZz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerZzZ</servlet-name>
        <servlet-class>Controlador.ControlerZzZ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarPersonalidadServlet</servlet-name>
        <servlet-class>Controlador.GuardarPersonalidadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControlerJ</servlet-name>
        <url-pattern>/ControlerJ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerZ</servlet-name>
        <url-pattern>/ControlerZ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerZz</servlet-name>
        <url-pattern>/ControlerZz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerZzZ</servlet-name>
        <url-pattern>/ControlerZzZ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarPersonalidadServlet</servlet-name>
        <url-pattern>/GuardarPersonalidadServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
